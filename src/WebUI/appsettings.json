{
  "UseInMemoryDatabase": false,
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=SharkoDb;Trusted_Connection=True;MultipleActiveResultSets=true;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "IdentityServer": {
    "Clients": {
      "Sharko.WebUI": {
        "Profile": "IdentityServerSPA",
        "RedirectUri": "/home",
        "LogoutUri": "/home"
      }
    }
  },
  "AllowedHosts": "*"
}
